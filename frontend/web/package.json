{
   "name": "@platform/web",
   "version": "0.0.0",
   "private": true,
   "type": "module",
   "scripts": {
      "dev": "vite",
      "build": "run-p type-check \"build-only {@}\" --",
      "preview": "vite preview",
      "test:unit": "vitest",
      "build-only": "vite build",
      "type-check": "vue-tsc --build",
      "lint": "eslint . --fix",
      "format": "prettier --write src/"
   },
   "exports": {
      ".": {
         "types": "./dist/types.d.ts",
         "import": "./dist/blank.js"
      },
      "./bits.css": "./dist/blank.css",
      "./fonts.css": "./dist/assets/styles/fonts.css",
      "./assets/fonts/*": "./dist/assets/fonts/*",
      "./assets/icons/*": "./dist/assets/icons/*",
      "./assets/images/*": "./dist/assets/images/*",
      "./dist/*": "./dist/*"
   },
   "files": [
      "dist"
   ],
   "main": "./dist/blank.js",
   "module": "./dist/blank.js",
   "types": "./dist/types.d.ts",
   "dependencies": {
      "@inertiajs/vue3": "^2.0.3",
      "@platform/blank": "workspace:*",
      "laravel-vite-plugin": "^1.2.0"
   }
}
