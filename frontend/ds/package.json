{
   "name": "@platform/ds",
   "version": "0.0.0",
   "private": true,
   "type": "module",
   "scripts": {
      "dev": "vite",
      "build": "run-p type-check \"build-only {@}\" --",
      "preview": "vite preview",
      "test:unit": "vitest --run",
      "build-only": "vite build",
      "type-check": "vue-tsc --build",
      "lint": "eslint . --fix",
      "format": "prettier --write src/"
   },
   "exports": {
      ".": {
         "types": "./dist/index.d.ts",
         "import": "./dist/ds.js"
      },
      "./bits.css": "./dist/bits.css",
      "./fonts.css": "./dist/assets/styles/fonts.css",
      "./assets/fonts/*": "./dist/assets/fonts/*",
      "./assets/icons/*": "./dist/assets/icons/*",
      "./assets/images/*": "./dist/assets/images/*",
      "./dist/*": "./dist/*"
   },
   "files": [
      "dist"
   ],
   "main": "./dist/ds.js",
   "module": "./dist/ds.js",
   "types": "./dist/index.d.ts",
   "dependencies": {
      "vue": "^3.5.13"
   },
   "devDependencies": {
      "vite-plugin-dts": "^4.5.0"
   }
}
